generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Message {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  chatId    String?
  createdAt DateTime @default(now()) @db.Date
  message   String?
  messageId String?
  type      String?
  updatedAt DateTime @default(now()) @db.Date
  username  String?
}
