generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

model messages {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  username  String
  chatId    String
  messageId String
  message   String
  type      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
